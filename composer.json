{
  "name": "onedesign/craft-hawksearch",
  "description": "Exports site content as a data feed tailored for Hawksearch integration.",
  "version": "1.0.0",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "loyolapress",
    "hawksearch"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "One Design Company",
      "homepage": "https://onedesigncompany.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/onedesign/craft-hawksearch",
    "docs": "https://github.com/onedesign/craft-hawksearch/blob/master/README.md",
    "email": "dev@onedesigncompany.com"
  },
  "require": {
    "craftcms/cms": "^3.1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "onedesign\\hawksearch\\": "src/"
    }
  },
  "extra": {
    "handle": "hawksearch",
    "name": "Hawksearch",
    "class": "onedesign\\hawksearch\\Hawksearch"
  }
}
